{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "RedisCache": {
        "Configuration": "localhost:6379",
        "InstanceName": "GoodSite"
    },
    "EasyCaching": {
        "redis": {
            "MaxRdSecond": 120,
            "EnableLogging": false,
            "LockMs": 5000,
            "SleepMs": 300,
            "dbconfig": {
                "Password": null,
                "IsSsl": false,
                "SslHost": null,
                "ConnectionTimeout": 5000,
                "AllowAdmin": true,
                "EndPoints": [
                    {
                        "Host": "localhost",
                        "Port": 6379
                    }
                ],
                "Database": 0
            }
        }
    }
}
